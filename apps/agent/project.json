{
  "name": "@sia/agent",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsx src/main.ts",
        "cwd": "apps/agent"
      },
      "cache": true
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/agent",
        "main": "apps/agent/src/main.ts",
        "tsConfig": "apps/agent/tsconfig.json",
        "assets": []
      }
    }
  }
}
