{
  "name": "@sia/landing-page",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/landing-page/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build",
        "cwd": "apps/landing-page"
      },
      "dependsOn": ["^build"],
      "cache": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite dev",
        "cwd": "apps/landing-page"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "apps/landing-page"
      }
    }
  }
}
